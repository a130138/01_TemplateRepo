{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters":
        {},
    "functions":
        [],
    "variables":
        {},
    "resources":
        [
            {
            "name": "$env:AzName",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2021-04-01",
            "tags":
                {
                "Owner": "$env:AzTagOwn",
                "Env": "$env:AzTagEnv"
                },
            "location": "$env:AzLoc",
            "kind": "$env:AzKind",
            "sku":
                {
                "name": "$env:AzSkuName",
                "tier": "$env:AzSkuTier"
                }
            }
        ],
    "outputs": {}
}
